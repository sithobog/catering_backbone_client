{{#each items}}
	<div class='item_in_order dish_id_in_order-{{id}}'>
		<span class='title_for_dish'>{{title}} X </span>
	  <span class='quantity_in_order'>{{quantity}}</span>
	  <span class='currency pull-right'>UAH</span>
	  <span class='price_for_dish pull-right'>{{math quantity "*" price}} </span>
	</div>
{{/each}}
